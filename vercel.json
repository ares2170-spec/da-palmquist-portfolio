{
  "version": 2,
  "functions": {
    "api/server.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "DB_NAME": "@db_name",
    "REACT_APP_BACKEND_URL": "https://da-palmquist-portfolio-2025-2-git-main-da-palmquists-projects.vercel.app"
  }
}
